{
	"name": "p5-project-creator",
	"displayName": "P5 Project Creator",
	"description": "Create default P5.js projects quickly and easily",
	"publisher": "ultamatum",
	"version": "1.2.0",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"icon": "res/p5js.png",
	"keywords": [
		"P5",
		"Processing",
		"Project Creation",
		"Project",
		"Default",
		"Quick"
	],
	"bugs": {
		"url": "https://github.com/ultamatum/P5-Project-Creator/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ultamatum/P5-Project-Creator.git"
	},
	"homepage": "http://ultamatum.net",
	"activationEvents": [
		"onCommand:p5-project-creator.createP5Project"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "p5-project-creator.createP5Project",
				"title": "Create P5 Project"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.15",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
